{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "lazy",
      "resources": {
        "files": [
          "/index.html",
          "/*.css"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "ytImages",
      "urls": [
        "https://i.ytimg.com/*"
      ],
      "cacheConfig": {
        "maxSize": 1000,
        "maxAge": "2d",
        "timeout": "5s",
        "strategy": "performance"
      }
    }
  ]
}